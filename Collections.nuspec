      <projectUrl>https://github.com/juergenpf/Collections</projectUrl>
      <licenseUrl>https://github.com/juergenpf/Collections/blob/master/License.md</licenseUrl>
      <tags>Collections BinaryHeap PriorityQueue</tags>
      <frameworkAssemblies>
            <frameworkAssembly assemblyName="System" targetFramework="" />
      </frameworkAssemblies>
      <releaseNotes>
Version 3 drops PCL builds in favour of .NET standard
      </releaseNotes>
